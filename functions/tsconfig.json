{
  "compilerOptions": {
    "lib": ["es2020", "dom"],
    "target": "ES2020",
    "module": "commonjs",

    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "compileOnSave": true,
  "include": ["src"]
}


