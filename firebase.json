{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules"
  },
  "emulators": {
    "functions": {
 "host": "127.0.0.1",
      "port": 5001
    },
    "firestore": {
 "host": "127.0.0.1",
 "port": 8081
    },
    "auth": {
 "host": "127.0.0.1",
 "port": 9100
    },
    "ui": {
 "host": "127.0.0.1",
 "port": 4000,
      "enabled": true
    },
    "hub": {
 "host": "127.0.0.1"
    },
    "eventarc": {
      "host": "127.0.0.1"
    },
    "tasks": {
      "host": "127.0.0.1"
    },
    "logging": {
 "host": "127.0.0.1"
    }
}
}
